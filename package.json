{
  "name": "thrift-generator",
  "version": "0.0.6",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "glob": "^8.0.3",
    "handlebars": "^4.7.7",
    "mkdirp": "^1.0.4",
    "winston": "^3.8.1"
  },
  "bin": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amirbilu/node-thrift-generator.git"
  },
  "keywords": [
    "thrift",
    "idl",
    "thrift-generator"
  ],
  "thrift-generator": {
    "log": "debug",
    "generators": [
      {
        "language": "java",
        "idl": "example/.thrift",
        "output": "./dist/java"
      },
      {
        "language": "js:node",
        "idl": "example/.thrift",
        "output": "./dist/node"
      }
    ]
  }
}
